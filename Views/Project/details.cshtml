@model IEnumerable<Tasks>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>description</th>
            <th>status</th>
            <th>priority</th>
            <th> deadline</th>
            <th> Team Member</th>
       
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Title</td>
                <td>@item.Description</td>
                <td>@item.Status</td>
                <td>@item.priority</td>
                <td>@item.DeadLine</td>
                <td>@item.TeamMember.Name</td>
                
            </tr>

        }


    </tbody>
</table>